# Database
DATABASE_URL=file:./dev.db

# JWT Authentication
JWT_MAX_AGE="7 Days"
JWT_SECRET=your_jwt_secret_here

# Redis
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=null

# BullMQ Dashboard
# WARNING: Do not enable publicly in production without auth
QUEUE_DASHBOARD_ENABLED=false

# Logging
LOGGING_ENABLED=false