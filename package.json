{
  "name": "elysia-api",
  "version": "1.0.51",
  "scripts": {
    "start": "bun run src/index.ts",
    "test": "bun test",
    "test:unit": "bun test tests/unit/",
    "test:feature": "bun test tests/feature/",
    "dev": "bun run --hot src/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit push",
    "db:seed": "bun run ./src/drizzle/seed.ts",
    "studio": "bun run drizzle-kit studio",
    "lint": "bun run --bun tsc --noEmit",
    "type-check": "bun run --bun tsc --noEmit",
    "format": "bun run --bun prettier --write .",
    "format:check": "bun run --bun prettier --check .",
    "audit": "bun audit"
  },
  "dependencies": {
    "@bogeychan/elysia-logger": "^0.1.9",
    "@elysiajs/bearer": "^1.3.0",
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/cron": "^1.3.0",
    "@elysiajs/swagger": "^1.3.1",
    "@libsql/client": "^0.15.10",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sentry/bun": "^9.44.2",
    "@yolk-oss/elysia-env": "^3.0.0",
    "argon2": "^0.43.1",
    "drizzle-orm": "^0.44.4",
    "elysia": "^1.3.8",
    "jsonwebtoken": "^9.0.2",
    "logixlysia": "^5.1.0",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@types/jsonwebtoken": "^9.0.10",
    "bun-types": "^1.2.19",
    "drizzle-kit": "^0.31.4"
  },
  "module": "src/index.js"
}
