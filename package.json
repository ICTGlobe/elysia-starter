{
  "name": "elysia-api",
  "version": "1.0.51",
  "scripts": {
    "start": "bun run src/index.ts",
    "test": "bun test",
    "test:unit": "bun test tests/unit/",
    "test:feature": "bun test tests/feature/",
    "dev": "bun run --hot src/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit push",
    "db:seed": "bun run ./src/drizzle/seed.ts",
    "studio": "bun run drizzle-kit studio",
    "lint": "bun run --bun tsc --noEmit",
    "type-check": "bun run --bun tsc --noEmit",
    "format": "bun run --bun prettier --write .",
    "format:check": "bun run --bun prettier --check .",
    "audit": "bun audit",
    "queue:work": "bun run src/queue/worker.ts",
    "make:job": "bun run src/cli/job-create.ts",
    "make:queue": "bun run src/cli/queue-create.ts",
    "make:event": "bun run src/cli/event-create.ts",
    "make:listener": "bun run src/cli/listener-create.ts",
    "queue:clear": "bun run src/cli/queue-clear.ts",
    "queue:list": "bun run src/cli/queue-list.ts",
    "queue:stats": "bun run src/cli/queue-stats.ts"
  },
  "dependencies": {
    "@bogeychan/elysia-logger": "^0.1.10",
    "@bull-board/api": "^6.16.2",
    "@bull-board/elysia": "^6.16.2",
    "@elysiajs/bearer": "^1.4.2",
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/cron": "^1.4.1",
    "@elysiajs/swagger": "^1.3.1",
    "@libsql/client": "^0.15.15",
    "@paralleldrive/cuid2": "^2.3.1",
    "@sentry/bun": "^9.47.1",
    "@yolk-oss/elysia-env": "^3.0.0",
    "argon2": "^0.43.1",
    "bullmq": "^5.66.4",
    "drizzle-orm": "^0.44.7",
    "elysia": "^1.4.21",
    "ioredis": "^5.9.0",
    "jsonwebtoken": "^9.0.3",
    "logixlysia": "^5.3.0",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@types/jsonwebtoken": "^9.0.10",
    "bun-types": "^1.3.5",
    "drizzle-kit": "^0.31.8"
  },
  "module": "src/index.js"
}
